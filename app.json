{
  "expo": {
    "name": "Fundite si Codite",
    "slug": "fsc2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/miau.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/miau.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.funditesicodite2.app",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/miau.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.funditesicodite.app",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "android": {
        "config": {
          "cleartextTrafficPermitted": true
        }
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "networkSecurityConfig": "./android/app/src/main/res/xml/network_security_config.xml"
          }
        }
      ]
    ]
  }
}
